    "t.set_title(\"Most % recoverd by continent\")\n",
    "t = sns.countplot(ax=ax[1], x=\"Continent\", data=df.nsmallest(20, \"% Deaths\"), order = df[\"Continent\"].unique(), color=\"blue\", saturation=0.3)\n",
    "t.set_title(\"Most % deaths by continent\")"
