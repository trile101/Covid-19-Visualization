    "t = sns.barplot(ax=ax[0], x=df[\"% Recovered\"], y='Country,Other', data=df.nlargest(20, \"% Recovered\"), color=\"blue\", saturation=0.3)\n",
    "t = sns.barplot(ax=ax[1], x=df[\"% Deaths\"], y='Country,Other', data=df.nsmallest(20, \"% Deaths\"), color=\"blue\", saturation=0.3)"
   ]
